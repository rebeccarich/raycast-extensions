{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dash-user-id",
  "title": "Dash User ID",
  "description": "Copy Dash user ID",
  "icon": "command-icon.png",
  "author": "rebeccarich1",
  "owner": "rebeccarich",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Copy Dash User ID",
      "description": "Copy Dash User ID",
      "mode": "no-view"
    }
  ],
  "preferences": [
    {
      "name": "userId",
      "description": "Dash user ID",
      "type": "textfield",
      "required": true,
      "title": "Dash user ID",
      "placeholder": "00000000-0000-0000-0000-000000000000"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.57.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
